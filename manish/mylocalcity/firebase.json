{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source" : "/examples/create",
        "destination" : "/#/examples/create"
      },
      {
        "source" : "/examples/list/:lat/:lng/:radius/:loc/:q/:page",
        "destination" : "/#/examples/list/:lat/:lng/:radius/:loc/:q/:page"
      },
      {
        "source" : "/examples/list/:page",
        "destination" : "/#/examples/list/:page"
      },
      {
        "source" : "/examples/list",
        "destination" : "/#/examples/list"
      }
    ]
  }
}
